{% extends 'base.html.twig' %}


{% block body %}

    {% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script>
        $(document).ready(function(){
            $("#myInput").on("keyup", function() {
                var value = $(this).val().toLowerCase();
                $("#myTable tr").filter(function() {
                    $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
                });
            });
        });


    </script>

    {% endblock %}


    <input id="myInput" type="text" placeholder="Search..">
    <br><br>

    <table>
        <thead>
        <tr>
            <th>Ime</th>
            <th>Prezime</th>
            <th>Naziv Projekta</th>
            <th>Aktivnost</th>
            <th>Utoseno vreme (sati)</th>
            <th>Datum</th>
        </tr>
        </thead>
    {% for key in userdats %}
        <tbody id="myTable">

        <tr>
            <td>{{ key.ime }}</td>
            <td>{{ key.prezime }} </td>
            <td>{{ key.nazivProjekta }} </td>
            <td>{{ key.aktivnost }}</td>
            <td>{{ key.utrosenoVreme }} </td>
            <td>*{{ key.datum|date("m/d/Y") }}</td>
        </tr>

        </tbody>



    {% endfor %}
    </table>

    {% for key in userdats %}

    {% endfor %}




{% endblock body %}

